{
  "name": "zapid",
  "version": "1.0.1",
  "description": "Generate unique, random IDs with built-in safety measures and collision probability warnings",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "ts-node build.ts && ts-node check-size.ts",
    "analyze": "ts-node check-size.ts",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "prepublishOnly": "npm test && npm run build",
    "dev": "tsc --watch"
  },
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "id",
    "generator",
    "unique",
    "random",
    "uuid",
    "identifier",
    "nanoid",
    "unique id"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Srikar Phani Kumar Marti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/srikarphanikumar/zapid"
  },
  "bugs": {
    "url": "https://github.com/srikarphanikumar/zapid/issues"
  },
  "homepage": "https://github.com/srikarphanikumar/zapid#readme",
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "@types/puppeteer": "7.0.4",
    "@types/serve-handler": "6.1.4",
    "esbuild": "^0.19.12",
    "rimraf": "^5.0.5",
    "jest": "29.7.0",
    "puppeteer": "21.11.0",
    "serve-handler": "6.1.6",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false
}